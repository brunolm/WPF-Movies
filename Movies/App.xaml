<Application x:Class="Movies.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Movies"
             xmlns:vm="clr-namespace:Movies.ViewModels"
             xmlns:vma="clr-namespace:Movies.ViewModels.Admin"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <vm:MainViewModel x:Key="MainViewModel" />
        <vma:UsersViewModel x:Key="UsersViewModel" />
        <vma:UserInputViewModel x:Key="UserInputViewModel" />
    </Application.Resources>
</Application>
